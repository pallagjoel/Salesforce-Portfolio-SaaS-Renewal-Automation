    <decisions>
        <description>this decision, checks if the opportunity, should be created for upsell or avoiding churn?</description>
        <name>churn_or_upsell</name>
        <label>churn or upsell?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Create_Renewal_Opportunity_Path_A</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Upsell</defaultConnectorLabel>
        <rules>
            <name>Churn</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Usage_Utilization__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Renewal_Opportunity_Path_A_2</targetReference>
            </connector>
            <label>Churn</label>
        </rules>
    </decisions>
