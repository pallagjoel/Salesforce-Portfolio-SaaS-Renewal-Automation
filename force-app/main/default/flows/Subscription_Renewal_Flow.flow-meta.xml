    <decisions>
        <description>This decision checks if an opportunity needs to be done before 30 days.</description>
        <name>I_subscription_healthy</name>
        <label>I subscription healthy?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_Open_Renewal_Opps_Path_A</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No, opportunity should be created</defaultConnectorLabel>
        <rules>
            <name>Yes_is_healthy</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Usage_Utilization__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Usage_Utilization__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>80.0</numberValue>
                </rightValue>
            </conditions>
            <label>Yes is healthy</label>
        </rules>
