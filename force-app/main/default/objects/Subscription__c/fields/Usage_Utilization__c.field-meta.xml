<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Usage_Utilization__c</fullName>
    <description>This formula shows the percentage, of allready used space by accounts</description>
    <formula>IF (TEXT(Product_Tier__c) = &quot;Enterprise&quot;,
0,
Total_Storage_Consumption__c  /  Data_Limit__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Usage Utilization</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
